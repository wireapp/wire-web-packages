{
  "dependencies": {
    "@types/fs-extra": "9.0.6",
    "@types/node": "~14",
    "@types/platform": "1.3.3",
    "ansi-regex": "5.0.0",
    "fs-extra": "9.1.0",
    "logdown": "3.3.1",
    "platform": "1.3.6",
    "url-search-params-polyfill": "8.1.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "del-cli": "3.0.1",
    "jest": "26.6.3",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.0",
    "typescript": "4.0.3"
  },
  "description": "Collection of common components that are used across Wire web applications.",
  "files": [
    "src",
    "!src/**/*.test.*",
    "!src/**/!(*.d).ts"
  ],
  "keywords": [
    "commons",
    "utils"
  ],
  "license": "GPL-3.0",
  "main": "src/main/index",
  "name": "@wireapp/commons",
