  "scripts": {
    "build": "yarn build:node",
    "build:node": "tsc",
    "clean": "del-cli \"src/**/*{.js,.js.map,.d.ts}\" \"!src/**/*{.test.node.js,.test.js}\" \"!src/test/**/*\"",
    "dist": "yarn clean && yarn build",
    "test": "yarn jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch"
  },
  "version": "4.0.3"
