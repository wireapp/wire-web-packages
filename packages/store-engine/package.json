{
  "dependencies": {
    "@types/filesystem": "0.0.28",
    "@types/node": "10.0.4",
    "bro-fs": "0.4.0",
    "dexie": "2.0.3",
    "fs-extra": "6.0.0"
  },
  "devDependencies": {
    "cross-env": "5.1.4",
    "istanbul": "1.1.0-alpha.1",
    "jasmine": "3.1.0",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-diff-reporter": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "rimraf": "2.6.2",
    "typescript": "2.8.3",
    "webpack": "3.11.0"
  },
  "description": "Provides a generic storage layer for File, FileSystem, IndexedDB, Memory & LocalStorage.",
  "files": [
    "dist"
  ],
  "keywords": [
    "File",
    "FileSystem",
    "fs",
    "generic",
    "IndexedDB",
    "LocalStorage",
    "Memory",
    "persistence",
    "storage",
    "transient",
    "wrapper"
  ],
  "license": "GPL-3.0",
  "main": "./dist/commonjs/index.js",
  "name": "@wireapp/store-engine",
  "repository": "https://github.com/wireapp/wire-web-packages/tree/master/packages/store-engine",
  "scripts": {
    "build": "yarn build:node && yarn build:browser",
    "build:browser": "webpack --progress",
    "build:node": "tsc",
    "clear": "rimraf dist",
    "clear:coverage": "rimraf coverage",
    "coverage": "yarn dist && yarn clear:coverage && yarn coverage:node && yarn coverage:browser && yarn coverage:report",
    "coverage:browser": "yarn test:browser",
    "coverage:node": "cross-env JASMINE_CONFIG_PATH=jasmine.json istanbul cover --report html --dir ./coverage/node ../../node_modules/jasmine/bin/jasmine.js",
    "coverage:report": "istanbul report --root coverage --dir ./coverage/report --verbose",
    "dist": "yarn clear && yarn build",
    "test": "yarn test:node && yarn test:browser && yarn test:coverage",
    "test:browser": "webpack --progress && karma start",
    "test:coverage": "yarn coverage && istanbul check-coverage --branches 40 --functions 65 --lines 60 --statements 60",
    "test:node": "jasmine --config=jasmine.json",
    "test:project": "yarn dist && yarn test"
  },
  "types": "./dist/commonjs/index.d.ts",
  "version": "0.11.5"
}
