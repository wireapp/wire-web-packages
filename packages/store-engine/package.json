{
  "dependencies": {
    "@types/node": "~14"
  },
  "devDependencies": {
    "@types/jasmine": "3.8.2",
    "@types/karma": "6.3.1",
    "jasmine": "3.8.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "4.0.1",
    "karma-sourcemap-loader": "0.3.8",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "typescript": "4.4.2",
    "webpack": "4.46.0",
    "webpack-cli": "4.7.2"
  },
  "description": "Provides a storage wrapper API to unify access to different storage technologies.",
  "files": [
    "src",
    "!src/**/*.test.*",
    "!src/**/!(*.d).ts",
    "!src/main/test"
  ],
  "keywords": [
    "generic",
    "persistence",
    "storage",
    "transient",
    "wrapper"
  ],
  "license": "GPL-3.0",
  "main": "src/main/index",
  "name": "@wireapp/store-engine",
  "repository": "https://github.com/wireapp/wire-web-packages/tree/main/packages/store-engine",
  "scripts": {
    "build": "yarn build:node && yarn build:browser",
    "build:browser": "webpack --progress",
    "build:node": "tsc",
    "clean": "rimraf \"src/**/*.{js?(.map),d.ts}\"",
    "dist": "yarn clean && yarn build",
    "test": "yarn test:node && yarn test:browser",
    "test:browser": "karma start",
    "test:project": "yarn dist && yarn test",
    "test:node": "yarn build && nyc jasmine --config=jasmine.json"
  },
  "version": "4.9.7"
}
