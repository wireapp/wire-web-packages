{
  "description": "Implementation of Concise Binary Object Representation (CBOR) data format (RFC7049).",
  "devDependencies": {
    "@types/node": "9.6.6",
    "coveralls": "3.0.0",
    "cross-env": "5.1.4",
    "istanbul": "1.1.0-alpha.1",
    "jasmine": "3.1.0",
    "rimraf": "2.6.2",
    "webpack": "3.11.0"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "name": "@wireapp/cbor",
  "repository": "https://github.com/wireapp/wire-web-packages/tree/master/packages/cbor",
  "scripts": {
    "build:browser": "webpack --progress",
    "build:node": "tsc",
    "build": "yarn build:node && yarn build:browser",
    "clear": "rimraf dist",
    "coverage:html": "istanbul cover --report html jasmine",
    "coverage:lcov": "istanbul cover --report lcov jasmine",
    "dist": "yarn clear && yarn build",
    "test": "yarn test:node && yarn test:browser",
    "test:project": "yarn dist && yarn test",
    "test:browser": "exit 0",
    "test:node": "jasmine"
  },
  "types": "dist/index.d.ts",
  "version": "3.0.17"
}
