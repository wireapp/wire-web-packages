{
  "bin": {
    "wire-cli": "dist/index.js"
  },
  "dependencies": {
    "@wireapp/core": "17.8.15",
    "@wireapp/store-engine-fs": "2.4.1",
    "commander": "7.2.0",
    "dotenv": "10.0.0",
    "fs-extra": "10.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.11",
    "rimraf": "3.0.2",
    "typescript": "4.2.4"
  },
  "description": "Command-line interface for Wire's secure messaging platform.",
  "files": [
    "src",
    "!src/**/!(*.d).ts"
  ],
  "license": "GPL-3.0",
  "main": "src/index",
  "name": "@wireapp/cli-client",
  "repository": "https://github.com/wireapp/wire-web-packages/tree/main/packages/cli-client",
  "scripts": {
    "build": "yarn build:node",
    "build:node": "tsc",
    "clean": "rimraf \"src/**/*.{js?(.map),d.ts}\"",
    "dist": "yarn clean && yarn build",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "test": "yarn test:node",
    "test:project": "yarn dist && yarn test",
    "test:node": "exit 0"
  },
  "version": "8.2.15"
}
