{
  "dependencies": {
    "@types/node": "*",
    "@wireapp/core": "16.12.30",
    "uuidjs": "4.2.6"
  },
  "devDependencies": {
    "cross-env": "7.0.2",
    "del-cli": "3.0.1",
    "dotenv-defaults": "2.0.1",
    "jasmine": "3.5.0",
    "rimraf": "3.0.2",
    "ts-node": "8.6.2",
    "typescript": "4.0.3"
  },
  "files": [
    "src",
    "!src/demo",
    "!src/**/!(*.d).ts"
  ],
  "license": "GPL-3.0",
  "main": "src/index",
  "name": "@wireapp/bot-api",
  "repository": "https://github.com/wireapp/wire-web-packages/tree/master/packages/bot-api",
  "scripts": {
    "build": "yarn build:node",
    "build:node": "tsc",
    "clean": "del-cli \"src/**/*{.js,.js.map,.d.ts}\" \"!src/**/*{.test.node.js}\"",
    "demo:admin": "ts-node -r tsconfig-paths/register src/demo/admin.ts",
    "dist": "yarn clean && yarn build",
    "test": "yarn build && yarn test:node && yarn clean",
    "test:node": "jasmine --config=jasmine.json",
    "test:project": "yarn dist && yarn test"
  },
  "version": "7.11.31"
}
