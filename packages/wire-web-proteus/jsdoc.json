{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "exclude": [],
        "excludePattern": "(^|\\/|\\\\)_",
        "includePattern": ".+\\.js(doc)?$"
    },
    "opts": {
        "comments"      : true,
        "destination"   : "./dist/typings",
        "encoding"      : "utf8",
        "lenient"       : true,
        "recurse"       : true,
        "template"      : "./node_modules/tsd-jsdoc"
    }
}
