{
  "bin": {
    "wire-standup-bot": "dist/start.js"
  },
  "dependencies": {
    "@wireapp/core": "3.11.0"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "jasmine": "3.1.0",
    "rimraf": "2.6.2",
    "typescript": "2.9.2"
  },
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "name": "@wireapp/standup-bot",
  "repository": "https://github.com/wireapp/wire-web-packages/tree/master/packages/standup-bot",
  "scripts": {
    "build": "yarn build:node && yarn build:browser",
    "build:browser": "exit 0",
    "build:node": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "start": "cross-env NODE_DEBUG=@wireapp/standup-bot* node dist/start.js",
    "test": "yarn test:node && yarn test:browser",
    "test:browser": "exit 0",
