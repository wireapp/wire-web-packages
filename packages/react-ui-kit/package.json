{
  "name": "@wireapp/react-ui-kit",
  "version": "0.0.1",
  "dependencies": {
    "color": "2.0.1",
    "gulp-concat": "2.6.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "styled-components": "2.2.3",
    "styled-normalize": "2.2.1"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "browser-sync": "2.18.13",
    "eslint": "4.10.0",
    "eslint-config-prettier": "2.7.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "gulp": "3.9.1",
    "gulp-plumber": "1.1.0",
    "gulp-sass": "3.1.0",
    "husky": "0.14.3",
    "lint-staged": "4.3.0",
    "prettier": "1.8.1",
    "react-hot-loader": "3.1.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "lint-staged": {
    "*.{js,jsx}": ["eslint -c .eslintrc.yaml --ignore-path .prettierignore --fix", "git add"],
    "*.json": ["prettier --write", "git add"],
    "*.scss": ["prettier --write", "git add"]
  },
  "repository": "https://github.com/wireapp/wire-web-packages/tree/master/packages/react-ui-kit",
  "scripts": {
    "clear": "echo \"Error: no clear specified\" && exit 1",
    "dist": "echo \"Error: no dist specified\" && exit 1",
    "fix:config": "prettier --write '**/*.json'",
    "fix:script": "npm run test -- --fix",
    "fix:style": "prettier --write '**/*.scss'",
    "fix": "npm run fix:config && npm run fix:style && npm run fix:script",
    "precommit": "lint-staged",
    "start": "npm run start:demo",
    "start:static": "gulp",
    "start:demo": "webpack-dev-server",
    "test": "eslint -c .eslintrc.yaml --ignore-path .gitignore --ignore-path .prettierignore '**/*.{js,jsx}'"
  }
}
