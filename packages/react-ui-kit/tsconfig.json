  "compilerOptions": {
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "dist",
    "rootDir": "src",
    "strict": false,
    "types": ["node", "jest"]
  },
  "exclude": ["dist", "node_modules"],
  "extends": "../../tsconfig.json"
}
