{
  "dependencies": {
    "@types/node": "~14",
    "logdown": "3.3.1"
  },
  "devDependencies": {
    "@types/karma": "6.3.1",
    "cross-env": "7.0.3",
    "jasmine": "3.8.0",
    "karma": "6.3.16",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-diff-reporter": "2.0.1",
    "karma-sourcemap-loader": "0.3.8",
    "karma-spec-reporter": "0.0.32",
    "rimraf": "3.0.2",
    "typescript": "4.4.2",
    "webpack": "4.46.0",
    "webpack-cli": "4.7.2"
  },
  "description": "A Promise-based dynamic queue runner.",
  "files": [
    "src",
    "!src/**/!(*.d).ts",
    "!src/test"
  ],
  "license": "GPL-3.0",
  "main": "src/main/index",
  "name": "@wireapp/promise-queue",
  "repository": "https://github.com/wireapp/wire-web-packages/tree/main/packages/promise-queue",
  "scripts": {
    "build": "yarn build:node && yarn build:browser",
    "build:browser": "webpack --progress",
    "build:node": "tsc",
    "clean": "rimraf .tmp \"src/main/{!(*.test*).js,*.js.map,*.d.ts}\" \"src/main/test/{*.js.map,*.d.ts}\" \"src/main/!(test)/{!(*.test*).js,*.js.map,*.d.ts}\" \"src/main/**/{!(*.test*|AccountHelper|StoreHelper).js,*.js.map,*.d.ts}\" \"src/main/**/*.node.js\"",
    "dist": "yarn clean && yarn build",
    "test": "yarn build && yarn test:node && yarn test:browser",
    "test:browser": "webpack && karma start",
    "test:project": "yarn dist && yarn test",
    "test:node": "cross-env JASMINE_CONFIG_PATH=src/test/node/support/jasmine.json jasmine"
  },
  "version": "1.0.0"
}
