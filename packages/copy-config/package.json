{
  "bin": "src/main/cli.js",
  "dependencies": {
    "axios": "1.1.3",
    "copy": "0.3.2",
    "cosmiconfig": "7.0.1",
    "fs-extra": "10.1.0",
    "jszip": "3.10.1",
    "logdown": "3.3.1"
  },
  "description": "Wire's internal configuration copy tool",
  "devDependencies": {
    "@types/copy": "0.3.2",
    "@types/fs-extra": "9.0.13",
    "jasmine": "4.4.0"
  },
  "files": [
    "src",
    "!src/**/*.test.*",
    "!src/**/!(*.d).ts"
  ],
  "license": "GPL-3.0",
  "main": "src/main/index",
  "name": "@wireapp/copy-config",
  "repository": "https://github.com/wireapp/wire-web-packages/tree/main/packages/copy-config",
  "scripts": {
    "build": "yarn build:node",
    "build:node": "tsc",
    "clean": "rimraf \"src/**/{!(*.test).js,*.js.map,*.d.ts}\"",
    "dist": "yarn clean && yarn build",
    "test": "yarn test:node",
    "test:node": "yarn build && jasmine --config=jasmine.json"
  },
  "version": "1.3.9"
}
