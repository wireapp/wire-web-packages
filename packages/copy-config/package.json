{
  "bin": {
    "copy-config": "src/main/cli.js"
  },
  "dependencies": {
    "axios": "0.21.1",
    "copy": "0.3.2",
    "cosmiconfig": "7.0.0",
    "fs-extra": "9.1.0",
    "jszip": "3.5.0",
    "logdown": "3.3.1",
    "rimraf": "3.0.2"
  },
  "description": "Wire's internal configuration copy tool",
  "devDependencies": {
    "@types/copy": "0.3.1",
    "@types/fs-extra": "9.0.6",
    "@types/rimraf": "3.0.0",
    "del-cli": "3.0.1",
    "jasmine": "3.6.3",
    "typescript": "4.0.3"
  },
  "files": [
    "src",
    "!src/**/*.test.*",
    "!src/**/!(*.d).ts"
  ],
  "license": "GPL-3.0",
  "main": "src/main/index",
  "name": "@wireapp/copy-config",
  "repository": "https://github.com/wireapp/wire-web-packages/tree/master/packages/copy-config",
  "scripts": {
    "build": "yarn build:node",
    "build:node": "tsc",
    "clean": "del-cli \"src/**/*{.js,.js.map,.d.ts}\" \"!src/**/*{.test.js}\"",
    "dist": "yarn clean && yarn build",
    "test": "yarn test:node",
    "test:node": "yarn build && jasmine --config=jasmine.json"
  },
  "version": "1.1.13"
}
