{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "target": "es5"
  },
  "exclude": ["node_modules", "src/**/*.test.ts", "src/**/*.test.tsx", "src/test/**/*"],
  "include": ["src/**/*"]
}
